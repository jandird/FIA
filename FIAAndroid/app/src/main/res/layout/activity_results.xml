<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.dalipjandir.fiaandroid.ResultsActivity"
        android:background="@drawable/mainback">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="345dp"
        android:layout_height="164dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:fontFamily="cursive"
        android:text="Is This Your Flag?"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:layout_width="300dp"
        android:layout_height="50dp" android:id="@+id/buttonF1" app:layout_anchor="@+id/title"
        app:layout_anchorGravity="center_vertical|center_horizontal"
        tools:layout_constraintTop_creator="1" tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1" android:layout_marginStart="35dp"
        app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="35dp"
        app:layout_constraintRight_toRightOf="parent" tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="35dp" android:layout_marginRight="35dp"
        app:layout_constraintVertical_bias="0.448" style="@style/Widget.AppCompat.Button"
        android:visibility="visible" android:alpha="100"
        android:textColor="@android:color/black"
        android:text="None of These"/>
    <Button
    android:layout_height="50dp"
    android:id="@+id/buttonF2" android:layout_width="300dp"
    app:layout_constraintTop_toBottomOf="@+id/buttonF1"
    android:layout_margin="10dp"
    android:layout_marginTop="20dp" android:layout_marginLeft="8dp"
    app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
    app:layout_constraintRight_toRightOf="parent" android:textSize="40sp"
        app:layout_constraintHorizontal_bias="0.492"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:layout_height="50dp"
        android:id="@+id/buttonF3" android:layout_width="300dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonF2"
        android:layout_margin="10dp"
        android:layout_marginTop="10dp" android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" android:textSize="40sp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
    <Button
        android:layout_height="50dp"
        android:id="@+id/buttonF4" android:layout_width="300dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonF3"
        android:layout_margin="10dp"
        android:layout_marginTop="10dp" android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" android:textSize="40sp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
    <Button
        android:layout_height="50dp"
        android:id="@+id/buttonF5" android:layout_width="300dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonF4"
        android:layout_margin="10dp"
        android:layout_marginTop="10dp" android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" android:textSize="40sp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />
    <Button
        android:layout_height="50dp"
        android:id="@+id/buttonF6" android:layout_width="300dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonF5"
        android:layout_margin="10dp"
        android:layout_marginTop="10dp" android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" android:textSize="40sp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Button
        android:id="@+id/buttonNone"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="10dp"
        android:text="None of These"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonF6"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

</android.support.constraint.ConstraintLayout>
